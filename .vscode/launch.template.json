  "configurations": [
    {
      "name": "Python: Test",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--ff",
        "--log-cli-level=INFO",
        "--pdb",
        "--setup-show",
        "--durations=5",
        "-sx",
        "-vv",
        "${file}"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: Testit",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": [
        "--ff",
        "--log-cli-level=INFO",
