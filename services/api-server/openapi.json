            "type": "string",
            "description": "Job creation timestamp",
            "format": "date-time"
          },
          "runner_name": {
            "title": "Runner Name",
            "pattern": "^([^\\s/]+/?)+$",
            "type": "string",
            "description": "Runner that executes job"
          },
          "url": {
            "title": "Url",
            "maxLength": 2083,
            "minLength": 1,
            "type": "string",
            "description": "Link to get this resource (self)",
            "format": "uri"
          },
          "runner_url": {
            "title": "Runner Url",
            "maxLength": 2083,
            "minLength": 1,
            "type": "string",
            "description": "Link to the solver's job (parent collection)",
            "format": "uri"
          },
          "outputs_url": {
            "title": "Outputs Url",
            "maxLength": 2083,
            "minLength": 1,
            "type": "string",
            "description": "Link to the job outputs (sub-collection",
            "format": "uri"
          }
        },
