{
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "build",
      "outputPath": "build-output",
      "bundle": {
        "include": [
          "qx.*"
        ]
      }
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "applications": [
    {
      "default": true,
      "class": "osparc.Application",
      "theme": "osparc.theme.products.osparc.ThemeDark",
      "name": "osparc",
      "title": "oSPARC",
      "include": [
        "iconfont.material.Load",
        "iconfont.fontawesome5.Load",
        "osparc.theme.products.osparc.ThemeLight"
      ],
      "environment": {
        "product.name": "osparc"
      },
      "addTimestampsToUrls": true,
      "bootPath": "source/boot"
    },
    {
      "class": "osparc.Application",
      "theme": "osparc.theme.products.s4l.ThemeDark",
      "name": "s4l",
      "title": "Sim4Life - ZMT",
      "include": [
        "iconfont.material.Load",
        "iconfont.fontawesome5.Load",
        "osparc.theme.products.s4l.ThemeLight"
      ],
      "environment": {
        "product.name": "s4l"
      },
      "addTimestampsToUrls": true,
      "bootPath": "source/boot"
    },
    {
      "class": "osparc.Application",
      "theme": "osparc.theme.products.s4l.ThemeDark",
      "name": "s4llite",
      "title": "S4L lite",
      "include": [
        "iconfont.material.Load",
        "iconfont.fontawesome5.Load",
        "osparc.theme.products.s4l.ThemeLight"
      ],
      "environment": {
        "product.name": "s4llite"
      },
      "addTimestampsToUrls": true,
      "bootPath": "source/boot"
    },
    {
      "class": "osparc.Application",
      "theme": "osparc.theme.products.s4l.ThemeDark",
      "name": "s4lacad",
      "title": "S4L Academic",
      "include": [
        "iconfont.material.Load",
        "iconfont.fontawesome5.Load",
        "osparc.theme.products.s4l.ThemeLight"
      ],
      "environment": {
        "product.name": "s4lacad"
      },
      "addTimestampsToUrls": true,
      "bootPath": "source/boot"
    },
    {
      "class": "osparc.Application",
      "theme": "osparc.theme.products.tis.ThemeDark",
      "name": "tis",
      "title": "TI Plan - IT'IS",
      "include": [
        "iconfont.material.Load",
        "iconfont.fontawesome5.Load",
        "osparc.theme.products.tis.ThemeLight"
      ],
      "environment": {
        "product.name": "tis"
      },
      "addTimestampsToUrls": true,
      "bootPath": "source/boot"
    }
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "globals": {
      "JSZip": false
    },
    "extends": [
      "@qooxdoo/qx/browser"
    ]
  },
  "$schema": "https://raw.githubusercontent.com/qooxdoo/qooxdoo-compiler/master/source/resource/qx/tool/schema/compile-1-0-0.json"
}
